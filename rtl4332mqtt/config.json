{
  "name": "RTL_433 to MQTT Bridge",
  "version": "0.4",
  "slug": "rtl4332mqtt",
  "description": "433MHz RF to MQTT Bridge based on RTL_SDR/RTL_433 for RTL2832U based DVB-T USB tuners",
  "url": "https://github.com/bobiboy/hassio-addons",
  "startup": "before",
  "udev":true,
  "boot": "auto",
  "map": ["config:rw", "ssl"],
  "devices": ["/dev/bus/usb/001/009:/dev/bus/usb/001/009:rwm"],
  "host_network": "False",
  "stage":"experimental",
  "arch": [
    "armv7"
  ],
  "options":
  {
    "mqtt_host": "homeassistant.local",
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_topic": "homeassistant/sensor/currentcost",
    "protocol": 100,
    "frequency": 319500000,
    "gain": 60,
    "frequency_offset": 350
  },
  "schema":
  {
    "mqtt_host": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_topic": "str",
    "protocol": "int",
    "frequency": "int",
    "gain": "int",
    "frequency_offset": "int"
   }
}
